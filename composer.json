{
  "name": "openactive/dataset-site",
  "description": "Library for rendering the OpenActive dataset site",
  "type": "project",
  "license": "MIT",
  "require": {
    "php": ">=5.5",
    "mustache/mustache": "^2.12",
    "openactive/models": "^1.0.0 | 0.*"
  },
  "autoload": {
    "psr-4": {
      "OpenActive\\DatasetSiteTemplate\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OpenActive\\DatasetSiteTemplate\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7 | ^9.0",
    "yoast/phpunit-polyfills": "^1.0"
  },
  "scripts": {
    "start": "php -S localhost:8080 -t examples/openactive",
    "start-csp": "php -S localhost:8080 -t examples/openactive-csp",
    "test": "./vendor/bin/phpunit"
  }
}
